    "keywords": ["react","twig","drupal"],
    "license": "GPL-2.0-or-later",
    "authors": [
	{
	    "name": "Tom Bennett",
	    "email": "tombennett86@hotmail.co.uk",
	    "homepage": "http://www.tombennett.info",
